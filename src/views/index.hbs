<script src="static/js/attach-pfp.js" defer></script>
<script src="static/js/navbar.js" defer></script>
<script src="static/js/establishments.js" defer></script>
<script src="static/js/card-view.js" defer></script>
<script src="static/js/authentication.js" defer></script>
<link rel="stylesheet" href="/static/css/style.css">
<link rel="stylesheet" href="/static/css/index.css">

<nav-component></nav-component> 
<nav-modal></nav-modal>
<div class="container cards">
    <h3 class="pt-4 pb-2">Food establishments near Taft</h3>
    <div class="row gy-5">
        {{#each establishments}}
            <div class="col-lg-3">
                <a href="/{{this._id}}">
                    <div class="content">
                        <div class="img-parent"><img src="{{this.profilePicture}}" alt=""></div>
                        <div class="mt-3">
                            <div class="d-flex justify-content-between">
                                <span class="bold">{{this.displayedName}}</span>
                                <div>
                                    <i class="fas fa-star me-2"></i><span class="regular">{{this.rating}}</span>
                                </div>
                            </div>
                            <span class="subtext">{{this.tag1}}<br>{{this.tag2}}</span>
                        </div>
                    </div>
                </a>
            </div>
        {{/each}}
    </div>
</div>
<footer>
</footer>
